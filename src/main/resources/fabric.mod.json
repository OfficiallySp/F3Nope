{
	"schemaVersion": 1,
	"id": "f3nope",
	"version": "${version}",
	"name": "F3Nope",
	"description": "Completely remove the F3 debug menu and replace it with custom text of your choosing. Fully configurable via commands or config file.",
	"authors": [
		"Your Name"
	],
	"contact": {
		"homepage": "https://github.com/yourname/f3nope",
		"sources": "https://github.com/yourname/f3nope"
	},
	"license": "MIT",
	"icon": "assets/f3nope/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.f3nope.F3Nope"
		],
		"client": [
			"com.f3nope.F3NopeClient"
		]
	},
	"mixins": [
		"f3nope.mixins.json",
		{
			"config": "f3nope.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
	}
}
